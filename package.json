{
  "name": "experiments-api",
  "version": "3.6.2",
  "description": "Experiments API",
  "main": "app.js",
  "scripts": {
    "build-server": "babel -d ./build src && babel ./src/swagger --out-dir ./build/swagger --copy-files && babel ./akana --out-dir ./build --copy-files && babel ./src/graphql --out-dir ./build/graphql --copy-files && babel ./migration-scripts --out-dir ./build/migration-scripts --copy-files && babel ./sqlMigration.js --out-dir ./build --copy-files",
    "clean": "rm -rf build && mkdir build && rm -rf doc",
    "debug": "cross-env NODE_ENV=development nodemon --debug ./src/app.js",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.js",
    "devDebug": "cross-env NODE_ENV=development nodemon $NODE_DEBUG_OPTION ./src/app.js",
    "eslint": "./node_modules/.bin/eslint src --fix --ext .js; exit 0",
    "eslintTests": "eslint test --fix --ext .js; exit 0",
    "migrations": "node ./sqlMigration.js",
    "precommit": "npm run validateSchema && eslint src --ext .js",
    "prepush": "jest --config .jestrc.json",
    "start": "NODE_ENV=production node ./build/app",
    "test": "jest --config .jestrc.json --detectOpenHandles",
    "testCoverage": "jest --config .jestCoveragerc.json --detectOpenHandles",
    "testDev": "jest --config .jestrc.json --watch",
    "validateSchema": "node ./src/graphql/scripts/verifySchema.js",
    "validateSwagger": "node ./src/swagger/validateSwagger.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.platforms.engineering:Cosmos/experiments-api.git"
  },
  "dependencies": {
    "@monsantoit/cloud-foundry": "3.0.0",
    "@monsantoit/oauth-ping": "4.4.0",
    "@monsantoit/error-decorator": "1.0.1",
    "@monsantoit/ping-page": "2.0.5",
    "@monsantoit/prom-metrics": "3.0.1",
    "@monsantoit/pg-transactional": "1.0.1",
    "avsc": "5.2.3",
    "aws-cloudfront-sign": "2.2.0",
    "aws-sdk": "2.294.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "console-timestamp": "0.3.0",
    "cors": "2.8.4",
    "dataloader": "1.4.0",
    "express": "4.16.3",
    "express-graphql": "0.6.12",
    "graphql": "0.13.2",
    "graphql-import": "0.6.0",
    "graphql-tools": "3.0.5",
    "graphql-type-json": "0.2.1",
    "json-inflector": "1.1.0",
    "lodash": "4.17.11",
    "log4js": "3.0.2",
    "kafka-node": "3.0.1",
    "pg-promise": "8.5.5",
    "promise-timeout": "1.3.0",
    "superagent": "3.8.3",
    "swagger-tools": "0.10.4",
    "uuid": "3.3.2",
    "validator": "10.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.2.0",
    "eslint": "5.2.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jest-async": "1.0.3",
    "husky": "0.14.3",
    "jest": "23.6.0",
    "nodemon": "1.18.3",
    "pg-monitor": "1.0.0"
  },
  "engines": {
    "node": "8.11.3"
  },
  "license": "ISC"
}
