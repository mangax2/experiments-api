{
  "name": "experiments-api",
  "version": "2.11.1",
  "description": "Experiments API",
  "main": "app.js",
  "scripts": {
    "build-server": "babel -d ./build src && babel ./src/swagger --out-dir ./build/swagger --copy-files && babel ./akana --out-dir ./build --copy-files",
    "clean": "rm -rf build && mkdir build",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.js",
    "devDebug": "cross-env NODE_ENV=development nodemon $NODE_DEBUG_OPTION ./src/app.js",
    "eslint": "./node_modules/.bin/eslint src --fix --ext .js; exit 0",
    "eslintTests": "eslint test --fix --ext .js; exit 0",
    "precommit": "eslint src --ext .js",
    "prepush": "jest --config .jestrc.json",
    "start": "NODE_ENV=production node ./build/app",
    "test": "jest --config .jestrc.json",
    "testCoverage": "jest --config .jestCoveragerc.json",
    "testDev": "jest --config .jestrc.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.platforms.engineering:Cosmos/experiments-api.git"
  },
  "dependencies": {
    "@monsantoit/cloud-foundry": "2.1.0",
    "@monsantoit/oauth-ping": "4.1.0",
    "@monsantoit/ping-page": "2.0.5",
    "@monsantoit/prom-metrics": "3.0.1",
    "aws-cloudfront-sign": "2.2.0",
    "bluebird": "3.4.7",
    "body-parser": "1.16.1",
    "compression": "1.6.2",
    "console-timestamp": "0.3.0",
    "express": "4.14.1",
    "json-inflector": "1.1.0",
    "lodash": "4.17.4",
    "log4js": "2.3.12",
    "no-kafka": "3.2.3",
    "pg-promise": "7.3.2",
    "promise-timeout": "^1.1.1",
    "superagent": "3.4.4",
    "swagger-tools": "0.10.1",
    "uuid": "3.1.0",
    "validator": "9.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "5.1.1",
    "eslint": "4.12.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.4.2",
    "eslint-plugin-jest-async": "1.0.3",
    "husky": "0.13.3",
    "jest": "21.2.1",
    "nodemon": "1.11.0",
    "pg-monitor": "0.7.1"
  },
  "engines": {
    "node": "~6.9.4",
    "npm": "3.10.x"
  },
  "license": "ISC"
}
