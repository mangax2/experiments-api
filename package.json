{
  "name": "experiments-api",
  "version": "1.2.2",
  "description": "Experiments API",
  "main": "app.js",
  "scripts": {
    "build-server": "babel -d ./build src && babel ./src/swagger --out-dir ./build/swagger --copy-files && babel ./akana --out-dir ./build --copy-files",
    "clean": "rm -rf build && mkdir build",
    "check-coverage": "nyc check-coverage --lines 90 --statements 90 --branches 90 --functions 90",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.js",
    "eslint": "./node_modules/.bin/eslint src; exit 0",
    "nyc": "nyc report --reporter=html",
    "precommit": "eslint src",
    "prepush": "node_modules/.bin/mocha --compilers js:babel-core/register test",
    "start": "NODE_ENV=production node ./build/app",
    "test": "node_modules/.bin/_mocha --compilers js:babel-core/register test",
    "testCoverage": "node_modules/.bin/nyc node_modules/.bin/_mocha test",
    "test:dev": "_mocha --watch test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.platforms.engineering:Cosmos/experiments-api.git"
  },
  "dependencies": {
    "@monsantoit/cloud-foundry": "~2.1.0",
    "@monsantoit/ping-page": "~2.0.3",
    "@monsantoit/profile-middleware": "~3.1.0",
    "bluebird": "~3.4.7",
    "body-parser": "~1.16.0",
    "compression": "~1.6.2",
    "console-timestamp": "~0.3.0",
    "es6-promise": "~4.0.5",
    "express": "~4.14.1",
    "express-promise": "~0.4.0",
    "hoek": "~4.1.0",
    "json-inflector": "~1.1.0",
    "lodash": "~4.17.4",
    "log4js": "~1.1.0",
    "pg-promise": "~5.5.6",
    "swagger-node-express": "~2.1.3",
    "swagger-tools": "~0.10.1",
    "validator": "~6.2.1",
    "superagent": "~3.4.1",
    "superagent-as-promised": "~4.0.0"
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-core": "~6.22.1",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-register": "~6.22.0",
    "babel-eslint": "~7.1.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "cross-env": "~3.1.4",
    "eslint": "~3.15.0",
    "husky": "~0.13.1",
    "istanbul": "~0.4.5",
    "mocha": "~3.2.0",
    "nodemon": "~1.11.0",
    "nyc": "~10.1.2",
    "pg-monitor": "~0.7.1",
    "piping": "~0.3.2",
    "sinon": "~1.17.7",
    "sinon-as-promised": "~4.0.2",
    "sinon-chai": "~2.8.0",
    "sql-generate": "~1.5.0",
    "supertest": "~3.0.0",
    "validate.js": "~0.11.1"
  },
  "engines": {
    "node": "~6.9.4",
    "npm": "3.10.x"
  },
  "license": "ISC"
}
