{
  "name": "experiments-api",
  "version": "1.9.1",
  "description": "Experiments API",
  "main": "app.js",
  "scripts": {
    "build-server": "babel -d ./build src && babel ./src/swagger --out-dir ./build/swagger --copy-files && babel ./akana --out-dir ./build --copy-files",
    "clean": "rm -rf build && mkdir build",
    "checkCoverage": "istanbul check-coverage --statement 90 --branch 90 --function 90 --line 90",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.js",
    "eslint": "./node_modules/.bin/eslint src --fix --ext .js; exit 0",
    "eslintFix": "./node_modules/.bin/eslint src --fix; exit 0",
    "precommit": "eslint src --ext .js",
    "prepush": "jest --config .jestrc.json",
    "start": "NODE_ENV=production node ./build/app",
    "test": "jest --config .jestrc.json",
    "testCoverage": "jest --config .jestCoveragerc.json",
    "testDev": "jest --config .jestrc.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.platforms.engineering:Cosmos/experiments-api.git"
  },
  "dependencies": {
    "@monsantoit/cloud-foundry": "~2.1.0",
    "@monsantoit/oauth-ping": "~3.0.1",
    "@monsantoit/ping-page": "~2.0.3",
    "@monsantoit/profile-middleware": "~3.1.0",
    "aws-cloudfront-sign": "~2.2.0",
    "aws-sdk": "~2.45.0",
    "bluebird": "~3.4.7",
    "body-parser": "~1.16.0",
    "compression": "~1.6.2",
    "console-timestamp": "~0.3.0",
    "es6-promise": "~4.0.5",
    "express": "~4.14.1",
    "express-promise": "~0.4.0",
    "hoek": "~4.1.0",
    "json-inflector": "~1.1.0",
    "lodash": "~4.17.4",
    "log4js": "~1.1.0",
    "markdown-it": "~8.3.1",
    "pg-promise": "~5.5.6",
    "superagent": "~3.4.1",
    "superagent-as-promised": "~4.0.0",
    "swagger-node-express": "~2.1.3",
    "swagger-tools": "~0.10.1",
    "validator": "~6.2.1"
  },
  "devDependencies": {
    "babel-cli": "~6.22.2",
    "babel-core": "~6.22.1",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-register": "~6.22.0",
    "babel-eslint": "~7.1.1",
    "babel-plugin-transform-decorators-legacy": "~1.3.4",
    "cross-env": "~3.1.4",
    "eslint": "~3.19.0",
    "eslint-config-airbnb": "~14.1.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-jsx-a11y": "~4.0.0",
    "eslint-plugin-react": "~6.9.0",
    "husky": "~0.13.1",
    "istanbul": "~0.4.5",
    "jest": "~19.0.2",
    "nodemon": "~1.11.0",
    "pg-monitor": "~0.7.1",
    "piping": "~0.3.2",
    "sql-generate": "~1.5.0",
    "supertest": "~3.0.0",
    "validate.js": "~0.11.1"
  },
  "engines": {
    "node": "~6.9.4",
    "npm": "3.10.x"
  },
  "license": "ISC"
}
